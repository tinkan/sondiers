<%- model_class = Prestation -%>
<div class="page-header">
 
</div>
<table class="table table-striped">
  <thead>
    <tr>
	  <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:projet) %></th>
      <th><%= model_class.human_attribute_name(:production) %></th>
      
	  <th><%= model_class.human_attribute_name(:commentaire) %></th>
	  <th><%= model_class.human_attribute_name(:category) %></th>
	</tr>
  </thead>
  <tbody>
    <% @prestations.each do |prestation| %>
      <tr>
       
        <td><%= prestation.date %></td>
        <td><%= prestation.projet %></td>
        <td><%= prestation.production %></td>
        <td><%= prestation.commentaire %></td>
		 <td><%= prestation.category_id %></td>
		
      </tr>
    <% end %>
  </tbody>
</table>

